{
  "name": "user_snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "language": [
      {
        "id": "goctl",
        "aliases": [
          "goctl"
        ],
        "extensions": [
          ".api"
        ],
        "configuration": "./language-configuration-goctl.json"
      }
    ],
    "snippets": [
      {
        "language": "go",
        "path": "./init/go.json"
      },
      {
        "language": "proto",
        "path": "./init/proto3.json"
      },
    ]
  }
}

