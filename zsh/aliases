# Common Configurations
alias cls='clear'
alias grep='grep --color=auto'
alias ll='ls -la'

# kubernetes && docker
alias kbgp='kubectl get pods'
alias kb='kubectl'
alias kl='kubectl logs'
alias kdp='kubectl describe pods'
alias dc='docker'
alias dps='docker ps -a'
alias dis='docker images -a'

alias python3=/usr/local/bin/python3.10
alias mysql=/usr/local/mysql/bin/mysql

# Some IDE
alias code=/usr/local/bin/code
alias goland=/usr/local/bin/goland
alias as='open -a /Applications/Android\ Studio.app'
alias idea='open -a /Applications/IntelliJ\ IDEA.app'

# Some Projects
alias aic='cd ~/workspace/golang/src/github.com/apisix-ingress-controller'
